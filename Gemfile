source "https://rubygems.org"

# The github-pages gem includes the jekyll version that is compatible with GitHub Pages
# along with a set of dependencies/plugins supported by GitHub Pages.
gem "github-pages", group: :jekyll_plugins

# Include cache plugin, recommended to be in the :jekyll_plugins group
# for compatibility with GitHub Pages.
gem "jekyll-include-cache", group: :jekyll_plugins

# WEBrick is needed for serving the site locally with Jekyll on Ruby 3.0 and newer.
gem 'webrick', '~> 1.7'
